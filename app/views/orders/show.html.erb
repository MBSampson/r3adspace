<div class="contain-fluid">
  <div class="container order-content-container">
    <p>
      <strong>Address:</strong>
      <%= @order.address %>
    </p>

    <p>
      <strong>State:</strong>
      <%= @order.state %>
    </p>

    <p>
      <strong>Zipcode:</strong>
      <%= @order.zipcode %>
    </p>

    <p>
      <strong>City:</strong>
      <%= @order.city %>
    </p>

    <%= link_to 'Edit', edit_order_path(@order) %> |
    <%= link_to 'Back', orders_path %>
  </div>
</div>
